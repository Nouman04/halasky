    "make:migration": "npx sequelize-cli migration:generate --migrations-path src/migrations --name",
    "db:migrate": "npx sequelize-cli db:migrate --config src/config/config.json --migrations-path src/migrations ",
    "migrate:one": "npx sequelize-cli db:migrate --config src/config/config.json  --migrations-path src/migrations  --name",
    "migrate:rollback": "npx sequelize-cli db:migrate:undo:all --config src/config/config.json --migrations-path src/migrations ",
    "migrate:rollback:one": "npx sequelize-cli db:migrate:undo --config src/config/config.json --migrations-path src/migrations  --name",
    "make:seeder": "npx sequelize-cli seed:generate --seeders-path src/seeders --name",
    "db:seed": "npx sequelize-cli db:seed:all --config src/config/config.json --seeders-path src/seeders ",
    "seed:one": "npx sequelize-cli seed:one --config src/config/config.json --seeders-path src/seeders  --seed"
